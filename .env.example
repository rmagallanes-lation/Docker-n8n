# Database Configuration
POSTGRES_USER=n8n
POSTGRES_PASSWORD=your_secure_password_here
POSTGRES_DB=n8n

# n8n Authentication
N8N_BASIC_AUTH_USER=admin
N8N_BASIC_AUTH_PASSWORD=your_secure_password_here

# n8n Encryption (generate a random string)
N8N_ENCRYPTION_KEY=your_random_encryption_key_here

# OpenWebUI Secret
WEBUI_SECRET_KEY=your_random_secret_key

# Timezone
TIMEZONE=America/Los_Angeles

# n8n Host Configuration (update for your domain)
N8N_PROTOCOL=https
N8N_HOST=your-domain.trycloudflare.com
N8N_PORT=5678
WEBHOOK_URL=https://your-domain.trycloudflare.com/
N8N_BASIC_AUTH_ACTIVE=true

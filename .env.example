# Database Configuration
POSTGRES_USER=n8n
POSTGRES_PASSWORD=replace_with_strong_password
POSTGRES_DB=n8n

# n8n Authentication
N8N_BASIC_AUTH_ACTIVE=true
N8N_BASIC_AUTH_USER=admin
N8N_BASIC_AUTH_PASSWORD=replace_with_strong_password

# n8n Encryption
N8N_ENCRYPTION_KEY=replace_with_long_random_hex

# OpenWebUI Secret
WEBUI_SECRET_KEY=replace_with_strong_secret

# Cloudflare Tunnel (Docker token from Zero Trust)
CF_TUNNEL_TOKEN=replace_with_cloudflare_tunnel_token

# Timezone
TIMEZONE=America/Los_Angeles

# n8n Host Configuration
N8N_PROTOCOL=https
N8N_HOST=n8n-admin.lation.com.mx
N8N_EDITOR_BASE_URL=https://n8n-admin.lation.com.mx
N8N_PORT=5678
N8N_PROXY_HOPS=1
WEBHOOK_URL=https://n8n.lation.com.mx/
